upload:
ts-node ~/metaplex/js/packages/cli/src/candy-machine-v2-cli.ts upload -k ../wallet/sa8Ur62SfgowG5mMDGeYNXZvkawrCcFdfsNeXPbfBEB.json -cp config.json Assets


create:
ts-node ~/metaplex/js/packages/cli/src/candy-machine-v2-cli.ts create_candy_machine -k ../wallet/sa8Ur62SfgowG5mMDGeYNXZvkawrCcFdfsNeXPbfBEB.json -p 0.90

verify:
ts-node ~/metaplex/js/packages/cli/src/candy-machine-v2-cli.ts verify_upload -k ../wallet/sa8Ur62SfgowG5mMDGeYNXZvkawrCcFdfsNeXPbfBEB.json